{
  "expo": {
    "name": "5520FinalProject",
    "slug": "5520FinalProject",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/parteesplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#eb8117"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/parteesplash.png",
        "backgroundColor": "#eb8117"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
    [
      "expo-image-picker",
      {
        "cameraPermission": "The app needs access to your camera."
      }
    ]
    ]
  }
}
